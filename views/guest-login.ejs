        <script>
            // Load chat history when the page loads
            window.onload = () => {
                const chatHistory = JSON.parse(sessionStorage.getItem('chatHistory')) || [];
                chatHistory.forEach(chat => {
                    appendMessage(chat.message, chat.sender);
                });
            };
        
            // Function to toggle the chat window's visibility
            function toggleChatBubble() {
                const chatWindow = document.getElementById("chat-window");
                if (chatWindow.style.display === "none" || chatWindow.style.display === "") {
                    chatWindow.style.display = "flex";  // Show chat window
                } else {
                    chatWindow.style.display = "none";  // Hide chat window
                }
            }
        
            // Function to send message and handle API response
            function sendMessage() {
                const userMessage = document.getElementById("userInput").value.trim();
                if (!userMessage) return;
                appendMessage(userMessage, "user-message");
        
                document.getElementById("userInput").value = "";  // Clear input box
                
                // Store chat message in sessionStorage
                storeChatHistory(userMessage, "user-message");
        
                // Send the message to the backend
                fetch("/chat", {
                    method: "POST",
                    headers: { "Content-Type": "application/json" },
                    body: JSON.stringify({ message: userMessage })
                })
                .then(response => response.json())
                .then(data => {
                    const botReply = data.reply || "Sorry, something went wrong. Please try again.";
                    appendMessage(botReply, "bot-message");
        
                    // Store bot reply in sessionStorage
                    storeChatHistory(botReply, "bot-message");
                })
                .catch(error => {
                    console.error("Error:", error);
                    const errorMessage = "Sorry, something went wrong. Please try again.";
                    appendMessage(errorMessage, "bot-message");
        
                    // Store error message in sessionStorage
                    storeChatHistory(errorMessage, "bot-message");
                });
            }
        
            // Function to append message to the chat window
            function appendMessage(message, sender) {
                const messageContainer = document.createElement("p");
                const bubble = document.createElement("div");
                bubble.classList.add("chat-bubble", sender);
                bubble.textContent = message;
                messageContainer.appendChild(bubble);
                document.getElementById("chat-messages").appendChild(messageContainer);
                document.getElementById("chat-messages").scrollTop = document.getElementById("chat-messages").scrollHeight;
            }
        
            // Function to store chat history in sessionStorage
            function storeChatHistory(message, sender) {
                const chatHistory = JSON.parse(sessionStorage.getItem('chatHistory')) || [];
                chatHistory.push({ message, sender });
                sessionStorage.setItem('chatHistory', JSON.stringify(chatHistory));
            }
        </script>
        

        <script>
            document.addEventListener("DOMContentLoaded", function () {
                // ✅ Retrieve stored booking data from sessionStorage
                const bookingData = JSON.parse(sessionStorage.getItem("bookingData"));
        
                if (bookingData) {
                    // ✅ Display correct room number
                    document.getElementById("roomNumber").innerText = `YOUR ROOM NUMBER: ${bookingData.roomNumber}`;
        
                    // ✅ Show correct stay duration in OTP message
                    document.getElementById("otpMessage").innerText = 
                        `This OTP will be valid for ${bookingData.stayDuration} days, from ${bookingData.checkInDate} until ${bookingData.checkOutDate}.`;
                } else {
                    document.getElementById("roomNumber").innerText = "YOUR ROOM NUMBER: Not Found";
                }
            });
        
            function goHome() {
                window.location.href = "index.html";
            }
        
            function generateOTP() {
                return Math.floor(100000 + Math.random() * 900000); // Generates a 6-digit OTP
            }
        
            async function requestOTP() {
                const email = document.getElementById('email')?.value;
                const otpMessage = document.getElementById('otpMessage');
        
                if (!email) {
                    alert("Please enter your email.");
                    return;
                }
        
                const otp = generateOTP();
                console.log("Generated OTP:", otp); // Debugging
        
                // ✅ Store OTP in sessionStorage for later validation
                sessionStorage.setItem("otp", otp);
        
                // ✅ Retrieve stay duration & check-in/out dates from sessionStorage
                const bookingData = JSON.parse(sessionStorage.getItem("bookingData"));
        
                if (!bookingData) {
                    alert("Booking details not found!");
                    return;
                }
        
                const stayDuration = bookingData.stayDuration;
                const checkInDate = bookingData.checkInDate;
                const checkOutDate = bookingData.checkOutDate;
        
                try {
                    await emailjs.send("service_i91h8c2", "template_8mgufqc", {
                        reply_to: email,
                        passcode: otp,
                        expiry_time: `${stayDuration} days`,  // Example: "3 days"
                        expiry_date: `from ${checkInDate} until ${checkOutDate}`
                    });
        
                    otpMessage.style.display = "block";
                    otpMessage.innerText = `Your Digital Key is successfully sent to your mail!!`;
                    otpMessage.style.color = "green";
        
                } catch (error) {
                    console.error("Failed to send OTP:", error);
                    alert("Failed to send OTP. Please try again.");
                }
            }
        
            // Expose functions globally
            window.requestOTP = requestOTP;
            window.goHome = goHome;
        </script>
 
        
